---------------------------------------------------------------------------------------------------------------------------------------------------------------

#include <iostream>
using namespace std;
int main()
{

	int a, b;
	cout << "Enter Quantity Of Products" << endl;
	cin >> a;
	cout << "\nEnter Total Price of Products" << endl;
	cin >> b;
	// if else condition
	if (a > 50){
		cout << "\nYou have achieved 20% Discount On Total Amount " << endl;
		cout << "Total Price = " << b - (b*0.2) << endl;
	}
	else{
		cout << "No Discount \n Total Price = " << b << endl;

	}
	return 0;
}


