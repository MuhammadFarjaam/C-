------------------------------------------------------------------------------------------------------------------------------------------------

#include <iostream>
#include <iomanip>
using namespace std;
int main(){
	int a, b, c, n_1000, n_500, n_100, n_50, n_20, n_10;
	cout << "Program To Number of notes to Customer";
	cout << "\n\nEnter Total Amount = ";
	cin >> a;
	cout << "\nEnter Given Amount = ";
	cin >> b;
	c = b - a;

	cout << "\n\n****************************************************";
	// if Conditions 
	if (c >= 1000)
	{
		n_1000 = c / 1000;
		c -= n_1000 * 1000;
		cout << setw(10)<< "\n1000 = " << n_1000 << endl;
	}
	if (c >= 500)
	{
		n_500 = c / 500;
		c -= n_500 * 500;
		cout << setw(10) << "\n500 = " << n_500 << endl;
	}
	if (c >= 100)
	{
		n_100 = c / 100;
		c -= n_100 * 100;
		cout << setw(10) << "\n100 = " << n_100 << endl;
	}
	if (c >= 50)
	{
		n_50 = c / 50;
		c -= n_50 * 50;
		cout << setw(10) << "\n50 = " << n_50 << endl;
	}
	if (c >= 20)
	{
		n_20 = c / 20;
		c -= n_20 * 20;
		cout << setw(10) << "\n20 = " << n_20 << endl;
	}
	if (c >= 10)
	{
		n_10 = c / 10;
		c -= n_10 * 5;
		cout << setw(10) << "\n10 = " << n_10 << endl;
	}

	return 0;
}

