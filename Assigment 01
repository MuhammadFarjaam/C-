#include <iostream>
using namespace std;
int main()
{
	/*Muhammad Farjaam
	  02-133192-086*/
	
	float a;
	cout << "A program that convert Pakistani Rupees into US dollars and UAE Darham";
	cout << "\n\nEnter Amount in PKR ";
	cin >> a; // Input from User
	// Taking a = 1000
	// Conversion
	cout << "\n\nAmount in US dollars = " << a*(0.006369) << " US $";
	cout << "\nAmount in UAE Darham = " << a*(1 / 42.99) << " AED" << endl;
	return 0;
}

